{
  "manifest_version": 3,
  "name": "Solid Devtools",
  "author": "Damian Tarnawski",
  "minimum_chrome_version": "94",
  "devtools_page": "src/devtools_page.html",
  "permissions": [
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/index.ts"
      ],
      "run_at": "document_start"
    }
  ]
}