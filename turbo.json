{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "typecheck": {
      "cache": false
    },
    "deploy": {
      "dependsOn": [
        "build",
        "test",
        "typecheck"
      ]
    },
    "publish": {
      "dependsOn": [
        "build",
        "test",
        "typecheck"
      ]
    }
  }
}