{
  "name": "@solid-devtools/shared",
  "version": "0.6.1",
  "license": "MIT",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "contributors": [],
  "homepage": "https://github.com/thetarnav/solid-devtools/tree/main/packages/shared#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thetarnav/solid-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/thetarnav/solid-devtools/issues"
  },
  "keywords": [
    "solid",
    "devtools"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "jest --config jest.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./bridge": {
      "import": "./dist/bridge.js",
      "require": "./dist/bridge.cjs",
      "types": "./dist/bridge.d.ts"
    },
    "./cursor": {
      "import": "./dist/cursor.js",
      "require": "./dist/cursor.cjs",
      "types": "./dist/cursor.d.ts"
    },
    "./diff": {
      "import": "./dist/diff.js",
      "require": "./dist/diff.cjs",
      "types": "./dist/diff.d.ts"
    },
    "./graph": {
      "import": "./dist/graph.js",
      "require": "./dist/graph.cjs",
      "types": "./dist/graph.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts"
    },
    "./variables": {
      "import": "./dist/variables.js",
      "require": "./dist/variables.cjs",
      "types": "./dist/variables.d.ts"
    },
    "./serialize": {
      "import": "./dist/serialize.js",
      "require": "./dist/serialize.cjs",
      "types": "./dist/serialize.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "bridge": [
        "./dist/bridge.d.ts"
      ],
      "cursor": [
        "./dist/cursor.d.ts"
      ],
      "diff": [
        "./dist/diff.d.ts"
      ],
      "graph": [
        "./dist/graph.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ],
      "variables": [
        "./dist/variables.d.ts"
      ],
      "serialize": [
        "./dist/serialize.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "solid-js": "^1.4.8",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "solid-js": "^1.4.8"
  },
  "dependencies": {
    "@solid-primitives/event-bus": "^0.1.0",
    "@solid-primitives/utils": "^3.0.1",
    "solid-js": "^1.4.8",
    "type-fest": "^2.18.0"
  }
}
